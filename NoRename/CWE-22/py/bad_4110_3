


def snake_case(value: str) -> str:
    value = re.sub(([A-Z]{2,})([A-Z][a-z]|[ -_]|$), <function <lambda> at 0x7f5fe09820c0>, value.strip())
    value = re.sub((^|[ _-])([A-Z]), <function <lambda> at 0x7f5fe0983ec0>, value)
    return stringcase.snakecase(value)


def pascal_case(value: str) -> str:
    return stringcase.pascalcase(value)


def spinal_case(value: str) -> str:
    return stringcase.spinalcase(value)
