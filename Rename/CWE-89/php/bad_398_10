                    "setGridParam",
                    {
                        var_81: true,
                        postData: { "filters": filters }
                    }
                ).trigger("reloadGrid");
            }
        });');

    $layout->add_script('
        function fn_1()
        {
            $("
            {
                $("
            });

            $("
            $(".var_82").var_83({"position": "absolute"});
            $(".jAutochecklist_list").css($("
            $(".var_84").hide();
            $(".jAutochecklist_list").var_83({
                "border-radius": "8px",
