  if (var_62 != PAM_SUCCESS) { int *pret = malloc(sizeof(int)); *pret = var_62; pam_set_data(pamh, "rad_setcred_return", (void *) pret, _int_free); return var_62; };
 }






 request->code = PW_AUTHENTICATION_REQUEST;
