   pam_set_item(pamh, PAM_AUTHTOK, new_password);
  }
 }

 if (var_61 & PAM_DEBUG_ARG) {
  _pam_log(LOG_DEBUG, "password change %s", var_62==PAM_SUCCESS ? "succeeded" : "failed");
 }

