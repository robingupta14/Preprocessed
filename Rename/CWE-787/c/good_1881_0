  if (*addr == '.') {
   dot_count++;
   digit_count = 0;
  } else if (!isdigit(*addr)) {
   dot_count = 5;
  } else {
   digit_count++;
   if (digit_count > 3) {
    dot_count = 5;
   }
  }
  addr++;
 }
