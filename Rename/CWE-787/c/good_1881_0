   var_62 = var_57;
   goto error;
  }


  if (!(var_61 & PAM_SKIP_PASSWD)) {
   var_62 = rad_converse(pamh, PAM_PROMPT_ECHO_OFF, config.prompt, &password);
   if (var_62 != PAM_SUCCESS) {goto error; };

  }
