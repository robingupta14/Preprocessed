 var_62 = talk_radius(&config, request, response, password, NULL, config.retries + 1);
 if (var_62 != PAM_SUCCESS) {goto error; };

 if (var_54 & PAM_DEBUG_ARG) _pam_log(LOG_DEBUG, "Got RADIUS response code %d", response->code);

