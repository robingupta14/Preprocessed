                if (var_54 & PAM_DEBUG_ARG) _pam_log(LOG_DEBUG, "ignore last_pass, force_prompt set");
  var_62 = pam_get_item(pamh, PAM_AUTHTOK, (CONST void **) &password);
  if (var_62 != PAM_SUCCESS) {goto error; };
        }

