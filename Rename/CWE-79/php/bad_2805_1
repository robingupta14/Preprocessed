    protected function fn_5($var_222 = false)
    {
        if ($var_222) {
            if (-1 === $var_11->var_5) {
                return sprintf(
                    'AND fdg.group_id IN (%s)',
                    implode(', ', $var_11->var_6),
                    $var_11->var_5,
                    implode(', ', $var_11->var_6));
            } else {
                return sprintf(
                    'AND ( fdg.group_id IN (%s) OR (fdu.user_id = %d OR fdg.group_id IN (%s)) )',
                    implode(', ', $var_11->var_6),
                    $var_11->var_5,
                    implode(', ', $var_11->var_6)
                );
            }
        } else {
            if (-1 !== $var_11->var_5) {
                return sprintf(
                    'AND ( fdu.user_id = %d OR fdu.user_id = -1 )',
                    $var_11->var_5
                );
            } else {
                return sprintf(
                    'AND fdu.user_id = -1',
                    $var_11->var_5
                );
            }
        }
    }
