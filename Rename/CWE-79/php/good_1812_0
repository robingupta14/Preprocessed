        if (!isset($var_109[0])) {
            $var_109 = array($var_109);
        }

        $var_208 = count($var_109);
        $var_21 = '';
        for ($var_67 = 0; $var_67 < $var_208; $var_67++) {
            if (empty($var_109[$var_67]['url']) || empty($var_109[$var_67]['text'])) {
                continue;
            }
            if (!isset($var_109[$var_67]['target'])) {
                $var_109[$var_67]['target'] = '';
            }
            if (!isset($var_109[$var_67]['onclick'])) {
                $var_109[$var_67]['onclick'] = '';
            }
            if (!isset($var_109[$var_67]['title'])) {
                $var_109[$var_67]['title'] = '';
            }
            if (!isset($var_109[$var_67]['accesskey'])) {
                $var_109[$var_67]['accesskey'] = '';
            }
            $var_84 = isset($var_109[$var_67]['class'])
                ? $var_109[$var_67]['class']
                : 'widget';
            if ($var_67 > 0) {
                $var_21 .= ' | ';
            }
            $var_21 .= Horde::link($var_109[$var_67]['url'], $var_109[$var_67]['text'], $var_84, $var_109[$var_67]['target'], $var_109[$var_67]['onclick'], $var_109[$var_67]['title'], $var_109[$var_67]['accesskey']) . htmlspecialchars($var_109[$var_67]['text']) . '</a>';
        }

        return $var_21;
    }

