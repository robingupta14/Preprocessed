    private function fn_67(
        &$var_277, $var_131
    ) {

        $var_57 = $var_42->fn_0('fields_meta'); 

        if (empty($var_131)) {
            return null;
        }

        if (mb_strpos($var_131, '.') === false) {
            $var_403 = $var_42->fn_0('table');
            $var_404 = $var_131;
        } else {
            list($var_403, $var_404)
                = explode('.', $var_131);
        }

        $var_403 = Util::unQuote($var_403);
        $var_404 = Util::unQuote($var_404);

        
        
        $var_405 = false;

        foreach ($var_57 as $var_274 => $var_259) {
            if (($var_259->var_46 == $var_403) && ($var_259->var_145 == $var_404)) {
                $var_405 = $var_274;
                break;
            }
        }

        if ($var_405 === false) {
            return null;
        }

        
        $var_279 = $GLOBALS['dbi']->fetchRow($var_277);

        
        $var_267 = 'PMA_mimeDefaultFunction';
        $var_266 = $var_267;
        $var_325 = array();

        
        $var_259 = $var_57[$var_405];

        if (stristr($var_259->var_232, self::var_21)
            || ($var_259->var_232 == self::var_20)
        ) {

