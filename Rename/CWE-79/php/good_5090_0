    public function fn_65()
    {

        $var_136 = md5($var_42->fn_0('sql_query'));
        $var_349 = array();
        if (isset($_SESSION['tmpval']['query'][$var_136])) {
            $var_349 = $_SESSION['tmpval']['query'][$var_136];
        }

        $var_349['sql'] = $var_42->fn_0('sql_query');

        if (empty($var_349['repeat_cells'])) {
            $var_349['repeat_cells'] = $GLOBALS['cfg']['RepeatCells'];
        }

        
        
        if (PMA_isValid($_REQUEST['session_max_rows'], 'numeric')) {
            $var_349['max_rows'] = (int)$_REQUEST['session_max_rows'];
            unset($_REQUEST['session_max_rows']);
        } elseif ($_REQUEST['session_max_rows'] == self::var_32) {
            $var_349['max_rows'] = self::var_32;
            unset($_REQUEST['session_max_rows']);
        } elseif (empty($var_349['max_rows'])) {
            $var_349['max_rows'] = $GLOBALS['cfg']['MaxRows'];
        }

        if (PMA_isValid($_REQUEST['pos'], 'numeric')) {
            $var_349['pos'] = $_REQUEST['pos'];
            unset($_REQUEST['pos']);
        } elseif (empty($var_349['pos'])) {
            $var_349['pos'] = 0;
        }

        if (PMA_isValid(
            $_REQUEST['pftext'],
            array(
                self::var_11, self::var_10
            )
        )
        ) {
            $var_349['pftext'] = $_REQUEST['pftext'];
            unset($_REQUEST['pftext']);
        } elseif (empty($var_349['pftext'])) {
            $var_349['pftext'] = self::var_11;
        }

        if (PMA_isValid(
            $_REQUEST['relational_display'],
            array(
                self::var_23, self::var_24
            )
        )
        ) {
            $var_349['relational_display'] = $_REQUEST['relational_display'];
            unset($_REQUEST['relational_display']);
        } elseif (empty($var_349['relational_display'])) {
            
            
            
            $var_349['relational_display'] = $GLOBALS['cfg']['RelationalDisplay'];
        }

        if (PMA_isValid(
            $_REQUEST['geoOption'],
            array(
                self::var_26, self::var_27,
                self::var_25
            )
        )
        ) {
            $var_349['geoOption'] = $_REQUEST['geoOption'];
            unset($_REQUEST['geoOption']);
        } elseif (empty($var_349['geoOption'])) {
            $var_349['geoOption'] = self::var_25;
        }

        if (isset($_REQUEST['display_binary'])) {
            $var_349['display_binary'] = true;
            unset($_REQUEST['display_binary']);
        } elseif (isset($_REQUEST['display_options_form'])) {
            
            unset($var_349['display_binary']);
        } elseif (isset($_REQUEST['full_text_button'])) {
            
        } else {
            
            
            
            $var_349['display_binary'] = true;
        }

        if (isset($_REQUEST['display_blob'])) {
            $var_349['display_blob'] = true;
            unset($_REQUEST['display_blob']);
        } elseif (isset($_REQUEST['display_options_form'])) {
            
            unset($var_349['display_blob']);
        }

        if (isset($_REQUEST['hide_transformation'])) {
            $var_349['hide_transformation'] = true;
            unset($_REQUEST['hide_transformation']);
        } elseif (isset($_REQUEST['display_options_form'])) {
            
            unset($var_349['hide_transformation']);
        }

        
        
        
        unset($_SESSION['tmpval']['query'][$var_136]);
