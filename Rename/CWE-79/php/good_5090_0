    private function fn_63(
        $var_443, $var_444, $var_266, $var_325,
        $var_267, $var_259, $var_198 = array(), &$var_445 = null
    ) {

        $var_445 = false;
        $var_382 = '[' . $var_443;

        if (isset($var_444)) {

            $var_446 = mb_strlen($var_444, '8bit');
            $var_447 = Util::formatByteDown($var_446, 3, 1);
            $var_382 .= ' - ' . $var_447[0] . ' ' . $var_447[1];

        } else {

            $var_382 .= ' - NULL';
            $var_446 = 0;

        }

        $var_382 .= ']';

        
        if (gettype($var_266) === "object") {
            $var_448 = strpos(
                $var_266->getMIMESubtype(),
                'Octetstream'
            );
            $var_450 = strpos($var_266->getMIMEtype(), 'Text');
            if ($var_448
                || $var_450 !== false
            ) {
                
                
                $var_382 = pack("H*", bin2hex($var_444));
            }
        }

        if ($var_446 <= 0) {
            return($var_382);
        }

        if ($var_267 != $var_266) {
            $var_382 = $var_266->applyTransformation(
                $var_382,
                $var_325,
                $var_259
            );
            return($var_382);
        }

