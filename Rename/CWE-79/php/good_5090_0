            }

        } elseif (($_SESSION['tmpval']['max_rows'] == self::var_32)
            || ($var_114 > $var_389)
        ) {

            $var_409 = $_SESSION['tmpval']['pos'];
            $var_413  = $var_389 - 1;

        } else {

            $var_409 = $_SESSION['tmpval']['pos'];
            $var_413  = $var_114 - 1;

        }

        $var_46 = new class_1($var_42->fn_0('table'), $var_42->fn_0('db'));
        if ($var_46->isView()
            && ($var_389 == $GLOBALS['cfg']['MaxExactCountViews'])
        ) {

            $var_395 = Message::notice(
                __(
                    'This view has at least this number of rows. '
                    . 'Please refer to %sdocumentation%s.'
                )
            );

            $var_395->addParam('[doc@cfg_MaxExactCount]');
            $var_395->addParam('[/doc]');
            $var_415 = Util::showHint($var_395);

        } else {
            $var_415 = false;
        }

        $var_395 = Message::success(__('Showing rows %1s - %2s'));
        $var_395->addParam($var_409);

        if ($var_415 !== false) {
            $var_395->addParam('... ' . $var_415, false);
        } else {
            $var_395->addParam($var_413);
        }

        $var_395->addMessage('(');

        if ($var_415 === false) {

            if (isset($var_56) && ($var_56 != $var_389)) {
                $var_417 = Message::notice(
                    $var_387 . __('%1$d total, %2$d in query')
                );
                $var_417->addParam($var_389);
                $var_417->addParam($var_56);
