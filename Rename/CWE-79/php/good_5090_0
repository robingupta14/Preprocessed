        }

        $var_408 = mb_strtoupper(
            mb_substr(
                $var_408, 0, $GLOBALS['cfg']['LimitChars']
            ) . '...'
        );

        
        $GLOBALS['dbi']->dataSeek($var_277, 0);

        
        return ' [' . htmlspecialchars($var_404)
            . ': <strong>' . htmlspecialchars($var_406) . ' - '
