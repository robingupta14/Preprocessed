    private function fn_35(
        $var_130, $var_131, $var_211,
        $var_213, $var_132, $var_57, $var_209
    ) {
        $var_227 = "";
        
        $var_228 = $var_42->fn_38(
            $var_130, $var_131,
            $var_211, $var_213
        );
        $var_230 = $var_213;
        if ($var_131[0] == '' || !$var_228) {
            $var_231 = $var_131[0] == ''
                ? 0
                : count($var_131);
            $var_131[$var_231]
                = Util::backquote(
                    $var_230
                );
            $var_132[$var_231] = (preg_match(
                '@time|date@i',
                $var_57->var_232
            )) ? self::var_30 : self::var_29;

        }

        $var_131 = array_filter($var_131);
        $var_214 = null;
        foreach ($var_131 as $var_176=>$var_233) {
            
            
            $var_234 = ($var_176 == 0);
            $var_213 = $var_233;
            $var_235 = $var_211;
            
            
            if (mb_strpos($var_213, '.') !== false) {
                $var_236 = explode('.', $var_213);
                
                
