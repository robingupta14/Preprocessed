            $var_406 = $var_279[$var_405];
        }

        $var_406 = mb_strtoupper(
            mb_substr(
                $var_406, 0, $GLOBALS['cfg']['LimitChars']
            ) . '...'
        );

        
        $GLOBALS['dbi']->dataSeek($var_277, $var_42->fn_0('num_rows') - 1);
        $var_279 = $GLOBALS['dbi']->fetchRow($var_277);

        
        $var_259 = $var_57[$var_405];
