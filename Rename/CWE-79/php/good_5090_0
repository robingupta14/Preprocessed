            [mb_strtolower($var_42->fn_0('db'))]
            [mb_strtolower($var_42->fn_0('table'))]
            [$var_341];

        if (! is_array($var_343['link_param'])) {
            $var_342[$var_343['link_param']] = $var_340;
        } else {
            
            
            $var_344 = 'SELECT `' . $var_340 . '` FROM `'
                . $var_314[$var_343['link_param'][1]] . '`.`'
                . $var_314[$var_343['link_param'][2]] . '`';
            $var_342[$var_343['link_param'][0]] = $var_344;
        }

        $var_345 = strpos($var_343['default_page'], '?') ? '&' : '?';
        if (empty($var_343['link_dependancy_params'])) {
            return $var_343['default_page']
                . PMA_URL_getCommon($var_342, 'html', $var_345);
        }

        foreach ($var_343['link_dependancy_params'] as $var_346) {

            
            
            if (is_array($var_346['param_info'])) {
                $var_342[$var_346['param_info'][0]]
                    = $var_346['param_info'][1];
