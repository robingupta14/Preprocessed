            . __('Sort by key')
            . ': <select name="sql_query" class="autosubmit">' . "\n";

        $var_174 = false;
        $var_175 = (isset($var_130) ? $var_130 : '');

        foreach ($var_172 as $var_176) {

            $var_177 = '`'
                . implode('` ASC, `', array_keys($var_176->getColumns()))
                . '` ASC';

            $var_179 = '`'
                . implode('` DESC, `', array_keys($var_176->getColumns()))
                . '` DESC';

            $var_174 = $var_174
                || ($var_175 == $var_177)
                || ($var_175 == $var_179);

            if (preg_match(
                '@(.*)([[:space:]](LIMIT (.*)|PROCEDURE (.*)|'
                . 'FOR UPDATE|LOCK IN SHARE MODE))@is',
                $var_134, $var_180
            )) {
                $var_181 = $var_180[1];
                $var_182 = $var_180[2];
            } else {
                $var_181 = $var_134;
                $var_182 = '';
            }

            $var_167 .= '<option value="'
                . htmlspecialchars(
                    $var_181  . "\n"
                    . ' ORDER BY ' . $var_177
                    . $var_182
                )
                . '"' . ($var_175 == $var_177
                    ? ' selected="selected"'
                    : '')
                . '>' . htmlspecialchars($var_176->getName()) . ' (ASC)</option>';

            $var_167 .= '<option value="'
                . htmlspecialchars(
                    $var_181 . "\n"
                    . ' ORDER BY ' . $var_179
                    . $var_182
                )
                . '"' . ($var_175 == $var_179
                    ? ' selected="selected"'
                    : '')
                . '>' . htmlspecialchars($var_176->getName()) . ' (DESC)</option>';
        }

        $var_167 .= '<option value="' . htmlspecialchars($var_134)
            . '"' . ($var_174 ? '' : ' selected="selected"') . '>' . __('None')
            . '</option>'
            . '</select>' . "\n"
            . '</form>' . "\n";

        return $var_167;

    } 


    
