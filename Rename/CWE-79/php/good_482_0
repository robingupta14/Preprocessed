function wp_filter_kses( $var_74 ) {
	return addslashes( wp_kses( stripslashes( $var_74 ), current_filter() ) );
}


function wp_kses_data( $var_74 ) {
	return wp_kses( $var_74, current_filter() );
}


function wp_filter_post_kses( $var_74 ) {
	return addslashes( wp_kses( stripslashes( $var_74 ), 'post' ) );
}


function wp_kses_post( $var_74 ) {
	return wp_kses( $var_74, 'post' );
}


