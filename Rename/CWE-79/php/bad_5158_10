    public function testGetMulti($object, $method, $expected, $args = array())
    {
        if (method_exists($object, $method)) {
            $reflectionMethod = new ReflectionMethod($object, $method);
            $this->assertEquals(
                $expected,
                $reflectionMethod->invokeArgs($object, $args)
            );
        }
    }

    
