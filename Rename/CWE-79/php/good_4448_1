            return redirect()->back()
                ->withErrors($var_44)
                ->withInput($var_16);
        }
        $var_49 = [
            'image'       => $var_16['image'],
            'alias'       => $var_16['alias'],
            'category_id' => (int) $var_16['category_id'],
            'status'      => !empty($var_16['status']) ? 1 : 0,
            'sort'        => (int) $var_16['sort'],
            'store_id'    => session('adminStoreId'),
