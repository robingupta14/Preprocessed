	foreach( $var_256 as $var_112 ) {
		if( !isset( $t_stats[$var_112['id']] ) ) {
			$var_144[] = bug_row_to_object( bug_cache_database_result( $var_112 ) );
		} else {
			$var_144[] = bug_row_to_object( bug_cache_database_result( $var_112, $t_stats[$var_112['id']] ) );
		}
	}
	return $var_144;
}


function fn_25( $var_72, $var_260 = true ) {
	collapse_open( 'filter' );
	fn_26( $var_72, $var_260, true );
	collapse_closed( 'filter' );
	fn_26( $var_72, $var_260, false );
	collapse_end( 'filter' );
}


