		$var_146 = auth_get_current_user_id();
	} else {
		$var_146 = (int)$var_140;
	}

	
	if( !access_has_project_level( config_get( 'stored_query_use_threshold' ) ) ) {
		return $var_387;
	}

	
	
	
	db_param_push();
	$var_13 = 'SELECT * FROM {filters}
					WHERE (project_id=' . db_param() . '
						OR project_id=0)
					AND name!=\'\'
					AND (is_public = ' . db_param() . '
						OR user_id = ' . db_param() . ')
					ORDER BY is_public DESC, name ASC';
	$var_137 = db_query( $var_13, array( $var_15, true, $var_146 ) );

	while( $var_112 = db_fetch_array( $var_137 ) ) {
		$var_387[$var_112['id']] = $var_112['name'];
	}

	$var_387 = array_unique( $var_387 );
	asort( $var_387 );

	return $var_387;
}


