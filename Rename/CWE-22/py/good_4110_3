


def function_0(var_0: str) -> str:
    return re.sub([^\w _-]+, , var_0)


def function_1(var_0: str) -> str:
    var_0 = re.sub(([A-Z]{2,})([A-Z][a-z]|[ -_]|$), <function <lambda> at 0x7f073b55f560>, var_0.strip())
    var_0 = re.sub((^|[ _-])([A-Z]), <function <lambda> at 0x7f073b55f060>, var_0)
    return var_0


def function_2(var_0: str) -> str:
    return stringcase.snakecase(group_title(_sanitize(var_0)))


def function_3(var_0: str) -> str:
